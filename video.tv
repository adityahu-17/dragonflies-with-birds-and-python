cut {
    using "video_orig.mp4"
    start "00:00:00"
    duration "00:00:10"
    render "video_a.mp4"
}

cut {
    using "video_orig.mp4"
    start "00:02:00"
    duration "00:00:10"
    render "video_b.mp4"
}


glue {
     using "video_a.mp4" "video_b.mp4"
     render "video_glued.mp4"
}

apply {
     using "video_glued.mp4"
     fx "lehmann"
     render "video_lehmann.mp4"
}

apply {
     using "video_glued.mp4"
     fx "hflip" "vflip"
     render "video_lehmann.mp4"
}
